<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.BlastController">
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <JFXTextField fx:id="searchSpeciesField" promptText="Species" />
      <JFXTextField fx:id="searchField" promptText="Search" />
      <CheckBox fx:id="predictedCheckbox" mnemonicParsing="false" selected="true" text="Include predicted" />
        <TableView fx:id="hitTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="definitionColumn" prefWidth="75.0" text="Definition" />
                <TableColumn fx:id="queryCoverageColumn" prefWidth="75.0" text="Query coverage" />
                <TableColumn fx:id="hitCoverageColumn" prefWidth="75.0" text="Hit coverage" />
            </columns>
        </TableView>
        <Pane fx:id="alignmentPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
    </VBox>


</AnchorPane>
